{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "strict": true,
    "target": "es2017",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "dom"]
  },
  "include": ["src/**/*.ts"]
}
