{
  "prettier": {
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "printWidth": 80,
          "semi": true,
          "singleQuote": false,
          "trailingComma": "none",
          "bracketSpacing": true,
          "jsxBracketSameLine": false
        }
      }
    ],
    "no-semi": false,
    "replace-string-literals": false
  }
}
